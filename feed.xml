---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Jonathan Buchholz</title>
  <link href="https://jonathanbuchh.org/feed.xml" rel="self"/>
  <link href="https://jonathanbuchh.org/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://jonathanbuchh.org/</id>
  <author>
    <name>Jonathan Buchholz</name>
    <email>jonathan@buchh.org</email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="https://jonathanbuchh.org{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>https://jonathanbuchh.org{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
    <author>
      <name>Jonathan Buchholz</name>
    </author>
  </entry>
  {% endfor %}
</feed>
